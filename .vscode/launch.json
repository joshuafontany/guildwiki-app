{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug GuildWiki App",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "start:debug"
        ],
        "port": 9229,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "restart": true,
        "skipFiles": [],
        "sourceMaps": true,
        "outFiles": [],
        "autoAttachChildProcesses": true,
        "sourceMapPathOverrides": {
            "<node_internals>/$:/plugins/tiddlywiki/*": "${workspaceFolder}/node_modules/tiddlywiki/plugins/tiddlywiki/*"
        }
      }
    ]
  }
  